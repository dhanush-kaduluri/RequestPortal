{
  "_version": "1.65.0",
  "sap.app": {
    "id": "ust.com.prapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.15.7",
      "toolsId": "fc12fab2-0f49-4c54-a154-b0663a30e6fb"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/RequisitionService/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "manage-manage": {
          "semanticObject": "manage",
          "action": "manage",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.131.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ust.com.prapp.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "PurchaseRequisitionList",
          "target": "PurchaseRequisitionList"
        },
        {
          "pattern": "PurchaseRequisition({key}):?query:",
          "name": "PurchaseRequisitionObjectPage",
          "target": "PurchaseRequisitionObjectPage"
        },
        {
          "name": "PurchaseRequisition_ItemsObjectPage",
          "pattern": "PurchaseRequisition({key})/Items({ItemsKey}):?query:",
          "target": "PurchaseRequisition_ItemsObjectPage"
        }
      ],
      "targets": {
        "PurchaseRequisitionList": {
          "type": "Component",
          "id": "PurchaseRequisitionList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/PurchaseRequisition",
              "variantManagement": "Page",
              "navigation": {
                "PurchaseRequisition": {
                  "detail": {
                    "route": "PurchaseRequisitionObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "PurchaseRequisitionObjectPage": {
          "type": "Component",
          "id": "PurchaseRequisitionObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/PurchaseRequisition",
              "navigation": {
                "Items": {
                  "detail": {
                    "route": "PurchaseRequisition_ItemsObjectPage"
                  }
                }
              },
              "content": {
                "body": {
                  "sections": {
                    "InternalNotes": {
                      "template": "ust.com.prapp.ext.fragment.InternalNotes",
                      "position": {
                        "placement": "After",
                        "anchor": "OrderItems"
                      },
                      "title": "Internal Notes"
                    }
                  }
                }
              }
            }
          }
        },
        "PurchaseRequisition_ItemsObjectPage": {
          "type": "Component",
          "id": "PurchaseRequisition_ItemsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "navigation": {},
              "contextPath": "/PurchaseRequisition/Items"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
